<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}

        {% endblock %}

        {% block javascripts %}
            {% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}
    </head>
    <body>
        {% block body %}
        <header>
            <div class="container">
                <div class="gradient"></div>
                <img class="logo" src="img\logo.png"  alt="la imagen no se ha podido cargar"/>
                     <img class="banner" src="img\icono.png" alt="La img del user no se ha podido cargar" />
            </div>
        </header>
        <div class="containerBusqueda">
            <div class="input-wrapper">
                <div class="icon"> <img src="img\lupa.png" alt="busqueda"/>
                    <div class="icon-inner">  </div>
                </div>
                <input type="text" class="text" placeholder="Escribe un productor para buscar...">
            </div>
            <div class="action">
                <div class="action-icon"></div>
            </div>
        </div>

        {% endblock %}
    </body>
</html>





